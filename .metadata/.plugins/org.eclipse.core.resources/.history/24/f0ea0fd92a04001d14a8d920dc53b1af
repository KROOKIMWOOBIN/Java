package JAVA_TEST;

public class if_test
{
	public static void main(String[] args)
	{
		System.out.println(1==1);
		System.out.println(1==2);
		System.out.println('A'=='A');
		System.out.println('A'=='B');
		String a = "Hello World";
		String b = new String("Hello World");
		System.out.println(a==b);
	}
}
