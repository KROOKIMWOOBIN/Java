package Level_0;

public class pro_181878 {
	public int solution(String myString, String pat) {
        int answer = 0;
        
        for(int i = 0; i < myString.length(); i++) {
        	if(myString.equalsIgnoreCase(pat)) {
        		answer = 1;
        	}
        }
        
        return answer;
    }
}
