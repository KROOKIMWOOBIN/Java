package Level_1;

import java.util.*;

public class pro_12933 
{
	public static void main(String[] args) 
	{
		solution(1234);
	}
	public static long solution(long n) 
	{
        long answer = 0;
        
        ArrayList<Integer> al = new ArrayList<>();
        
        for(long i = n; i > 0; i/=10)
        {
        	al.add((int)i%10);
        }
        
        System.out.println(al.size());
        
        return answer;
    }
}
