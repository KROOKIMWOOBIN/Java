package JT_1_2_1;

public class Test1{
	
	public static void main(String[] args) {
		
		String dg = "Dongyang";
		
		for(int i = dg.length() - 1; i >= 0; i--) {
			if(Character.isUpperCase(dg.charAt(i))) {
				System.out.println(Character.toLowerCase(dg.charAt(i)));
			}
			else {
				System.out.println(Character.toUpperCase(dg.charAt(i)));
			}
		}
		
	}
	
}