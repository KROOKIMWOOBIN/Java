package Level_1;

public class pro_12947 
{
	public static boolean solution(int x) 
	{
        boolean answer = true;
        int sum = 0, ch = x;
        while(ch >= 1 && ch > 10) 
        {
        	sum += x % 10;
        	ch = ch / 10;
        }
        if(x / sum == 0)
        {
        	answer = true;
        }
        else
        {
        	answer = false;
        }
        
        return answer;
    }
	public static void main(String[] args) 
	{
		System.out.println(solution(10));
	}
}
