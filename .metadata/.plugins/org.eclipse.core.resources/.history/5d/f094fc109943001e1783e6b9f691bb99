package BAEKJOON;

import java.util.*;

public class problem_10815 
{
	static int haveCardList[] = new int[500001];
	static Scanner sc = new Scanner(System.in);
	public static void main(String[] args) 
	{
		int inputhaveCardList = sc.nextInt();
		for(int i = 0; i < inputhaveCardList; i++)
		{
			haveCardList[i] = sc.nextInt();
		}
		Arrays.sort(haveCardList);
		int inputCardList = sc.nextInt();
		for(int i = 0; i < inputCardList; i++)
		{
			int inputCard = sc.nextInt();
			System.out.print(cardSearch(0, inputhaveCardList - 1, inputCard));
		}
	}
	public static int cardSearch(int left, int right, int card)
	{
		if(left == right)
		{
			return card == haveCardList[left] ? 1 : 0;
		}
		int mid = left + right / 2;
		if(haveCardList[mid] == card)
		{
			return 1;
		}
		if(haveCardList[mid] > card)
		{
			return cardSearch(left, mid, card);
		}
		return cardSearch(mid + 1, right, card);
	}
}
