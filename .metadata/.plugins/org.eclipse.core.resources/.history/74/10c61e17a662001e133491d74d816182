package Level_0;

public class pro181939 {
	public int solution(int a, int b) {
        int answer = 0;
        
        String strA = Integer.toString(a) + Integer.toString(b);
        String strB = Integer.toString(b) + Integer.toString(a);
        
        if(strA.equals(strB)) {
        	answer = Integer.parseInt(strA);
        }
        else {
        	answer = Integer.parseInt(strB);
        }
        
        return answer;
    }
}
