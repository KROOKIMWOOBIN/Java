package Level_1;

public class pro_12947 
{
	public static boolean solution(int x) 
	{
        boolean answer = true;
        int sum = 0;
        while(x >= 1) 
        {
        	sum += x % 10;
        	x = x / 10;
        }
        System.out.println(x/ sum);
        if(x / sum == 0)
        {
        	answer = true;
        }
        else
        {
        	answer = false;
        }
        
        return answer;
    }
	public static void main(String[] args) 
	{
		System.out.println(solution(13));
	}
}
