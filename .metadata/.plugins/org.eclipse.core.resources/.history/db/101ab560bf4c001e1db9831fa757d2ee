package Level_2;

public class pro_42883 
{
	public static void main(String[] args) 
	{
		System.out.println(solution("1512", 2));
	}
	public static String solution(String number, int k) {
        
		String answer = "";
		StringBuffer sb = new StringBuffer();
        sb.append(number);
        answer = sb.deleteCharAt(0).toString();
        int max = Integer.parseInt(answer), min;
        for(int i = 0; i < k; i++)
        {
        	for(int j = 1; j < number.length(); j++)
            {
        		answer = sb.deleteCharAt(j).toString();
        		min = Integer.parseInt(answer);
        		if(max < min)
        		{
        			max = Integer.parseInt(answer);
        		}
            }
        }
        return answer = Integer.toString(max);
    }
}
