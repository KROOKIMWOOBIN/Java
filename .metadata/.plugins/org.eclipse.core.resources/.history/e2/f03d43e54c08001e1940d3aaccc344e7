package Level_1;

public class pro_12935 {

	public static int[] solution(int[] arr) {
        if(arr.length > 1) {
        	int[] answer = new int[arr.length - 1];
        }
        
        int min = arr[0];
        
        for(int i = 1; i < arr.length; i++) {
        	if(min > arr[i]) {
        		min = arr[i];
        	}
        }
        
        int j = 0;
        
        for(int i = 0; i < arr.length; i++) {
        	if(arr[i] != min) {
        		answer[j] = arr[j];
        		j++;
        	}
        }
        
        if(arr.length <= 1) {
        	answer[0] = -1;
        }
        
        return answer;
    }

}
