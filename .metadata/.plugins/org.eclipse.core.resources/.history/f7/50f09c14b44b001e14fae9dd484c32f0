package Level_2;

public class pro_154538 
{
	public int solution(int x, int y, int n) 
    {
        int dp[] = new int[y + 1];
        
        for(int i = 0; i <= y; i++)
        {
        	if(i != x && dp[i] == 0)
        	{
        		dp[y] = -1;
        		continue;
        	}
        	if(i * 2 <= y)
        	{
        		
        	}
        }
        
        return dp[y];
    }
}
