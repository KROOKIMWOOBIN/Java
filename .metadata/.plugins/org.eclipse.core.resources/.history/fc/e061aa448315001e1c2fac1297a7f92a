package Level_0;

public class pro_181874 {
	public String solution(String myString) {
        String answer = "";
        
        for(int i = 0; i < myString.length(); i++){
        	char a = myString.charAt(i);
        	if(a == 'a') {
        		answer += String.;
        	}
        
        }
        
        return answer;
    }
}
